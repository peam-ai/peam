{
  "name": "@peam-ai/docs-typecheck",
  "description": "Type-check documentation code samples",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts*\"",
    "test:docs": "vitest run --config vitest.config.ts",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:lint": "eslint",
    "test:format": "prettier --check \"src/**/*.ts*\""
  },
  "dependencies": {
    "peam": "workspace:*",
    "@peam-ai/client": "workspace:*",
    "@peam-ai/server": "workspace:*",
    "@peam-ai/next": "workspace:*",
    "ai": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.5.0",
    "@types/react": "^19.2.8",
    "eslint": "^9.18.0",
    "glob": "^11.0.0",
    "prettier": "^3.4.2",
    "react": "^19.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.20.0",
    "vitest": "^2.1.8"
  }
}
