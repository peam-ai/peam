{
  "name": "@peam/ai",
  "version": "0.1.0",
  "description": "AI abstractions and utilities for Peam",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://peam.ai",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "test": "jest"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.0",
    "@peam/logger": "*",
    "@peam/search": "*",
    "ai": "^6.0.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@peam/tsconfig": "*",
    "@types/node": "^22.10.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
