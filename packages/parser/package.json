{
  "name": "@peam/parser",
  "description": "Content parser for extracting page metadata",
  "private": true,
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://peam.ai",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test:eslint": "eslint \"src/**/*.ts*\"",
    "test:prettier": "prettier --check \"src/**/*.ts*\"",
    "test:jest": "jest",
    "test": "run-p test:*"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@peam/tsconfig": "*",
    "@types/jsdom": "^27.0.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "@peam/logger": "*",
    "jsdom": "^27.3.0",
    "matcher": "^6.0.0",
    "robots-parser": "^3.0.1"
  }
}
