{
  "name": "@peam/next",
  "description": "Peam Next.js integration",
  "private": true,
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://peam.ai",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "test:eslint": "eslint \"src/**/*.ts*\"",
    "test:prettier": "prettier --check \"src/**/*.ts*\"",
    "test:jest": "jest",
    "test": "run-p test:*"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./peam.adapter.js": "./dist/peam.adapter.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@peam/tsconfig": "*",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.9.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.50.0"
  },
  "dependencies": {
    "@peam/logger": "*",
    "@peam/parser": "*",
    "next": ">=15.0.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "peerDependencies": {
    "next": ">=15.0.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}
