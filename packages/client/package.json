    "nanoid": "^5.1.6",
    "shiki": "^3.20.0",
    "streamdown": "^2.0.1",
    "tailwind-merge": "^3.4.0",
    "tokenlens": "^1.3.1",
    "tw-animate-css": "^1.4.0",
    "use-stick-to-bottom": "^1.1.1"
