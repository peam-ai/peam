    "ai-elements": "^1.6.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.468.0",
