{
  "name": "@peam-ai/client",
  "version": "0.1.0",
  "description": "Client side components for Peam",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://peam.ai",
  "author": "Afshin Mehrabani",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/peam-ai/peam.git",
    "directory": "packages/client"
  },
  "bugs": {
    "url": "https://github.com/peam-ai/peam/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint ./src",
    "test:eslint": "eslint \"src/**/*.ts*\"",
    "test:prettier": "prettier --check \"src/**/*.ts*\"",
    "test:jest": "jest"
  },
  "files": [
    "dist/**/*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.0",
    "@ai-sdk/react": "^3.0.5",
    "@peam-ai/ai": "workspace:*",
    "@peam-ai/logger": "workspace:*",
    "@peam-ai/search": "workspace:*",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
