{
  "name": "@peam-ai/client",
  "version": "0.1.4",
  "description": "Client side components for Peam",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://peam.ai",
  "author": "Afshin Mehrabani",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/peam-ai/peam.git",
    "directory": "packages/client"
  },
  "bugs": {
    "url": "https://github.com/peam-ai/peam/issues"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*\"",
    "test:lint": "eslint \"src/**/*\"",
    "test:format": "prettier --check \"src/**/*\"",
    "test:unit": "vitest run"
  },
  "files": [
    "dist/**/*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.71",
    "@peam-ai/ai": "workspace:*",
    "@peam-ai/logger": "workspace:*",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@streamdown/cjk": "^1.0.1",
    "@streamdown/code": "^1.0.1",
    "@streamdown/math": "^1.0.1",
    "@streamdown/mermaid": "^1.0.1",
    "ai": "^6.0.69",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dexie": "^4.3.0",
    "dexie-react-hooks": "^4.2.0",
    "lucide-react": "^0.468.0",
    "motion": "^12.23.26",
    "nanoid": "^5.1.6",
    "radix-ui": "^1.4.3",
    "streamdown": "^2.1.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "use-stick-to-bottom": "^1.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-prefix-selector": "^2.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.5.1",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": "^18.0 || ^19.0",
    "react-dom": "^18.0 || ^19.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  }
}
